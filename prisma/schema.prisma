generator client {
provider = "prisma-client-js"
}


datasource db {
provider = "postgresql"
url = env("DATABASE_URL")
}


model User {
id String @id @default(cuid())
email String @unique
name String?
password String
createdAt DateTime @default(now())
books Book[]
}


model Book {
id String @id @default(cuid())
title String
author String
genre String?
createdAt DateTime @default(now())


userId String?
user User? @relation(fields: [userId], references: [id])
}